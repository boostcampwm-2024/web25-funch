{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@rtmp/*": ["core/rtmp/*"],
      "@media/*": ["core/media/*"],
      "@hls/*": ["core/hls/*"],
      "@/*": ["*"]
    },
    "noImplicitAny": false
  }
}
